# Sui Network Configuration
VITE_NETWORK=testnet
VITE_PACKAGE_ID=0x0

# Sui RPC Provider Configuration
# Current: AllThatNode (free, reliable)
VITE_RPC_PROVIDER=allthatnode

# Alternative RPC Providers (uncomment if you have API keys)
# QuickNode (Recommended for production)
# VITE_QUICKNODE_API_KEY=your_quicknode_api_key_here
# VITE_QUICKNODE_ENDPOINT=https://your-endpoint.sui-testnet.quiknode.pro

# Ankr (Alternative)
# VITE_ANKR_API_KEY=your_ankr_api_key_here

# IPFS Configuration
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key

# API Endpoints
VITE_API_BASE_URL=http://localhost:3001

# Feature Flags
VITE_ENABLE_TESTNET=true
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true